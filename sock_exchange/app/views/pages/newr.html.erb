<body>
  <div class="container">
    <div class="row">
      <div class="col-md-12">
        <div class="page-header">
          <h1>The Sock Exchange <p class="text-right"><small class="text-warning"><strong>Welcome, Sarah!</strong> | Log Off</small></p></h1>
        </div>
      </div>
    </div>

    <div class="row">
      <div class="col-md-12">
      <h4 class="text-primary">My Profile / My History / View Requests / <strong>New Request</strong></h4>
      </div>
    </div>

    <div class="row">
      <div class="col-md-12">
      <h3><strong>New Request</strong></h3>
      </div>
    </div>

    <form role="form">
      <div class="form-group">
        <label for="exampleInputItem">Item Name</label>
        <input type="Item" class="form-control" id="exampleInputItem" placeholder="Enter item name">
      </div>
      <div class="form-group">
        <label for="exampleInputItemDescription">Item Description</label>
        <input type="ItemDescription" class="form-control" id="exampleInputItemDescription" placeholder="Enter item descpription">
      </div>
      <div class="form-group">
        <label for="exampleInputBorrowDate">Borrow By Date</label>
        <input type="BorrowDate" class="form-control" id="exampleInputBorrowDate" placeholder="Enter borrow by date">
      </div>
      <div class="form-group">
        <label for="exampleInputReturnDate">Return By Date</label>
        <input type="ReturnDate" class="form-control" id="exampleInputReturnDate" placeholder="Enter return by date">
      </div>
      <div class="form-group">
        <label for="exampleInputFile">Upload Photo</label>
        <input type="file" id="exampleInputFile">
        <p class="help-block">Upload picture of ideal item.</p>
      </div>
      <button type="submit" class="btn btn-primary btn-large">Submit Request</button>
    </form>

  </div>
</body>